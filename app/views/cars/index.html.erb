<%= form_with url: search_path, method: :get do %>
  <%= label_tag :modelo, 'Busca:' %>
  <%= text_field_tag :modelo %>
  <%= submit_tag 'Pesquisar' %>
<% end %>

<ul>
  <% @cars.each do |car| %>
    <li>
      <%= link_to car.modelo, car %>
    </li>
  <% end %>
</ul>

<%= link_to "New Car", new_car_path %>

